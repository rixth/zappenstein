#stage
  - 5.times do |col_number|
    .col{:class => 'col-' + col_number.to_s}
      - 4.times do |slide_number|
        .slide.z{:id => 'col-' + col_number.to_s + '-slide-' + slide_number.to_s}
          .content
:javascript
  $LAB.script('//ajax.googleapis.com/ajax/libs/jquery/1.6.0/jquery.min.js')
    .script('/javascript/slide-manager.js')
    .script('/javascript/data-source.js')
    .wait()
    .script('/javascript/application.js');